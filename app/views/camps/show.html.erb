<div class="row">
  <div class="col s7">
    <%= render :partial => "camp_details" %>
  </div>


  <div class="col s5">
    <% if current_user && current_user.role?(:admin) %>
      <%= render :partial => "camp_instructors_vue"%>
    <%else%>
     <%= render :partial => "camp_instructors"%>
    <%end%>
    <%# render :partial => "camp_students"%>
  </div>

  <div class="col s5">
    <% if logged_in? %>
      <%= render :partial => "add_registration_vue"%>
    <% end %>
  </div>




  <div class="col s5">
    <% if logged_in? && (current_user.role?(:admin) || @camp.instructors.map{|i| i.user.id}.include?(current_user.id))%>
    <%# @camp.instructors.map{|i| i.user.id} %>
    <%#current_user.id %>
      <%= render partial: 'registrations' %>
    <% end %>
  </div>
</div>
<div class="row">
  <div class="col s12">
    <%= render :partial => "partials/object_options", locals: {model_name: "camp", object: @camp} %>
  </div>


</div>
